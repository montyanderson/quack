<!DOCTYPE html>
<html data-client-id="{{client_id}}" data-socket="{{socketsPort}}" data-port="{{port}}">
    <head>

        <meta charset="utf-8" />

        <title>quack</title>

        <link href="style.css" rel="stylesheet" />

    </head>
    <body>

        <nav>
            <h1>quack</h1>
        </nav>

        <section class="page page-login">
            <a class="login">Login with Instagram</a>
        </section>

        <section class="page page-main">
            <h1>Hello, <span class="username"></span>!</h1>

            <div id="chat"></div>

            <input class="text" maxlength="50" />
        </section>

        <script src="jquery.min.js"></script>
        <script src="mustache.min.js"></script>
        <script src="socket.io/socket.io.js"></script>
        <script src="client.js"></script>

    </body>
</html>
